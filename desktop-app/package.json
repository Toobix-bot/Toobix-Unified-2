{
  "name": "toobix-city-desktop",
  "version": "1.0.0",
  "description": "Toobix City - Native Desktop Application",
  "main": "dist/main.js",
  "scripts": {
    "start": "set ELECTRON_RUN_AS_NODE=0 && npm run build && npx electron .",
    "build": "tsc",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "build": {
    "appId": "com.toobix.city",
    "productName": "Toobix City",
    "copyright": "Copyright Â© 2025 Toobix",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer/**/*",
      "assets/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  },
  "keywords": [
    "toobix",
    "consciousness",
    "AI",
    "3D"
  ],
  "author": "Toobix Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "electron-store": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
